{
  "extends": "./tsconfig.json",
  "include": ["dist/src"],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "./dist/src",
    "paths": {
      "@tryfinch/finch-api/_shims/*": ["dist/src/_shims/*.node"],
      "@tryfinch/finch-api": ["dist/src/index.ts"],
      "@tryfinch/finch-api/*": ["dist/src/*"],
      "digest-fetch": ["./typings/digest-fetch"]
    },
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "pretty": true,
    "sourceMap": true
  }
}
